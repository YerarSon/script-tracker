{
  "name": "script-tracker",
  "type": "module",
  "displayName": "script-tracker",
  "description": "Save snapshots of your script with timestamp and optional description in JSON format.",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
  "commands": [
    {
      "command": "scriptTracker.saveSnapshot",
      "title": "Script Tracker: Guardar snapshot"
    },
    {
      "command": "scriptTracker.restoreSnapshot",
      "title": "Script Tracker: Restaurar snapshot"
    }
  ]
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  },
"repository": {
  "type": "git",
  "url": "https://github.com/YerarSon/script-tracker.git"
}



}
